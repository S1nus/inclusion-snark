{
    "$schema": "https://raw.githubusercontent.com/succinctlabs/succinctx/main/assets/schema.json",
    "entrypoints": [
        {
            "name": "succinct-alpha-demo",
            "framework": "plonky2x",
            "baseDir": ".",
            "buildCommand": "cargo run --package circuits --release build && mv ./target/release/circuits ./build/circuits",
            "proveCommand": "RUST_LOG=debug ./build/circuits prove input.json"
        }
    ]
}